    "@mui/styles": "^5.12.3",
    "@mui/system": "^5.12.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "18.15.11",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "apexcharts": "^3.39.0",
    "async-mutex": "^0.4.0",
    "dayjs": "^1.11.9",
    "env-cmd": "^10.1.0",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "firebase": "^9.21.0",
